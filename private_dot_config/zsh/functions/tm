# emulate -L zsh

tmux server-info
[[ $? -gt 0 ]] && return 1
[[ $# -eq 0 ]] && tmux list-sessions && return 0

local session="$1"

if [ -z "$TMUX" ]; then
    tmux new-session -As "${session}"
else
  if ! tmux has-session -t "${session}" 2> /dev/null; then
    TMUX= tmux new-session -ds "${session}"
  fi
  tmux switch-client -t "${session}"
fi

# vim: ft=zsh
